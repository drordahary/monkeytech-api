    "sequelize-cli": "^6.6.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  },
  "scripts": {
    "build": "tsc",
    "dev": "npm run db:prepare && ts-node-dev --respawn --transpile-only src/server.ts",
    "start": "npm run db:prepare && node dist/server.js",
    "db:prepare": "npm run db:migrate && npm run db:clean && npm run db:seed",
    "db:migrate": "sequelize-cli db:migrate",
    "db:clean": "sequelize-cli db:seed:undo:all",
    "db:seed": "sequelize-cli db:seed:all"
  }
